<Window x:Class="WpfYieldReturnApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Number Generator" Height="300" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Start Value:" />
            <TextBox x:Name="StartTextBox" Margin="0,0,0,10" />
            <TextBlock Text="End Value:" />
            <TextBox x:Name="EndTextBox" Margin="0,0,0,10" />
            <Button Content="Generate" Click="GenerateButton_Click" Margin="0,0,0,10" />
            <ListBox x:Name="NumbersListBox" />
        </StackPanel>
    </Grid>
</Window>